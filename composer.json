{
    "type": "library",
    "name": "nguyenanhung/uuid-helper",
    "description": "UUID Helper, basic, simple and light",
    "keywords": [
        "helper",
        "uuid",
        "php"
    ],
    "homepage": "https://github.com/nguyenanhung/uuid-helper",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nguyen An Hung",
            "email": "dev@nguyenanhung.com",
            "homepage": "https://nguyenanhung.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "paragonie/random_compat": ">=2.0"
    },
    "require-dev": {
        "ramsey/uuid": "^4 || ^3"
    },
    "autoload": {
        "psr-4": {
            "nguyenanhung\\Libraries\\UUID\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    }
}
